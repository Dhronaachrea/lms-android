<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/draw_res_main_lay"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_40sdp"
    android:minHeight="@dimen/_33sdp"
    android:padding="@dimen/_3sdp"
    android:weightSum="5">


    <com.skilrock.customui.CustomTextView
        android:id="@+id/hour"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_weight="0.3"
        android:background="@drawable/hour_bg"
        android:gravity="center"
        android:padding="1dp"
        android:text="@string/text"
        android:textColor="#fff"
        android:textSize="@dimen/fast_draw_res_list_row_corner_tx_sz" />

    <com.skilrock.customui.CustomTextView
        android:id="@+id/time"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1.4"
        android:gravity="center"
        android:text="25:25"
        android:textColor="#969696"
        android:textSize="@dimen/fast_draw_res_list_row_tx_sz" />


    <com.skilrock.customui.CustomTextView
        android:id="@+id/winner"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_alignRight="@id/time"
        android:layout_weight="1.2"
        android:gravity="center"
        android:text="10"
        android:textColor="#4C4C4C"
        android:textSize="@dimen/fast_draw_res_list_row_tx_sz" />


    <com.skilrock.customui.CustomTextView
        android:id="@+id/fast_res_text_gone"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0.7"
        android:gravity="center"
        android:text="EIGHT"
        android:textColor="@color/fast_ball_text_color"
        android:visibility="invisible" />

    <com.skilrock.customui.CustomTextView
        android:id="@+id/fast_res_num_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/_4sdp"
        android:layout_marginRight="@dimen/_8sdp"
        android:layout_weight="1.4"
        android:background="@drawable/fast_img_back"
        android:gravity="center"
        android:padding="@dimen/_3sdp"
        android:text="08\nEIGHT"
        android:textColor="#fff"
        android:textSize="@dimen/_12sdp" />

</LinearLayout>


